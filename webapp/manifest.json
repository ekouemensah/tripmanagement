{
  "_version": "1.48.0",
  "sap.app": {
    "id": "tripmanagement.tripmanagement",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.8.3",
      "toolsId": "8d96c4d8-f126-48a9-9509-769c7faf7c99"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZTRIPMANAGEMENT_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",          
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.119.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "tripmanagement.tripmanagement.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "defaultUpdateMethod": "PUT"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "tripmanagement.tripmanagement.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteHome",
          "pattern": ":?query:",
          "target": [
            "TargetHome"
          ]
        },
        {
          "name": "RouteTrip",
          "pattern": "RouteTrip/{Partenza},{Arrivo},{DataPartenza},{OraPartenza}",
          "target": [
            "TargetTrip"
          ]
        },
        {
          "name": "RouteTripDetails",
          "pattern": "RouteTripDetails/{ViaggioID},{Itinerari},{Prezzo},{Posto}",
          "target": [
            "TargetTripDetails"
          ]
        },
        {
          "name": "RouteTratte",
          "pattern": "RouteTratte",
          "target": [
            "TargetTratte"
          ]
        },
        {
          "name": "RouteFermate",
          "pattern": "RouteFermate",
          "target": [
            "TargetFermate"
          ]
        },
        {
          "name": "RouteBus",
          "pattern": "RouteBus",
          "target": [
            "TargetBus"
          ]
        },
        {
          "name": "RouteViaggi",
          "pattern": "RouteViaggi",
          "target": [
            "TargetViaggi"
          ]
        },
        {
          "name": "RouteStatistica",
          "pattern": "RouteStatistica",
          "target": [
            "TargetStatistica"
          ]
        }

      ],
      "targets": {
        "TargetHome": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Home",
          "viewName": "Home"
        },
        "TargetTrip": {         
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Trip",
          "viewName": "Trip"
        },
        "TargetFermate": {         
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Fermate",
          "viewName": "Fermate"
        }
        ,
        "TargetTripDetails": {         
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "TripDetails",
          "viewName": "TripDetails"
        },
        "TargetTratte": {         
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Tratte",
          "viewName": "Tratte"
        },
        "TargetBus": {         
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Bus",
          "viewName": "Bus"
        },
        "TargetViaggi": {         
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Viaggi",
          "viewName": "Viaggi"
        },
        "TargetStatistica": {         
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Statistica",
          "viewName": "Statistica"
        }
      }
    },
    "rootView": {
      "viewName": "tripmanagement.tripmanagement.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
